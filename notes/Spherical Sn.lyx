\begin_layout Standard
Use discrete ordinates 
\begin_inset Formula $\mu_{m}$
\end_inset

 with corresponding weights 
\begin_inset Formula $w_{m}$
\end_inset

 to simplify the solution, 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\int_{-1}^{1}\psi\left(r,\mu\right)d\mu\approx\sum_{m=1}^{M}w_{m}\psi_{m}\left(r\right),
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
using the angular differencing scheme from Lewis and Miller 
\begin_inset CommandInset citation
LatexCommand cite
key "lewis1984computational"

\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\mu_{m}\frac{\partial}{\partial r}\left(r^{2}\psi_{m}\left(r\right)\right)+\frac{r}{w_{m}}\left[\alpha_{m+1/2}\psi_{m+1/2}\left(r\right)-\alpha_{m-1/2}\psi_{m-1/2}\left(r\right)\right]+r^{2}\sigma\left(r\right)\psi_{m}\left(r\right)=r^{2}q_{m}\left(r\right),
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
with the following definitions of the half-ordinate quantities:
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{subequations}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{gather}
\alpha_{m}=\alpha_{m+1/2}+\alpha_{m-1/2},\\
\psi_{m}=\frac{\psi_{m+1/2}+\psi_{m-1/2}}{2},\\
\alpha_{1/2}=\alpha_{M+1/2}=0,\\
\alpha_{m+1/2}=\alpha_{m-1/2}-\mu_{m}w_{m}.
\end{gather}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{subequations}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
From these, each 
\begin_inset Formula $\psi_{m+1/2}$
\end_inset

 can be calculated, given that 
\begin_inset Formula $\psi_{m}$
\end_inset

 and 
\begin_inset Formula $\psi_{m-1/2}$
\end_inset

 have already been calculated.
 This is made possible by defining a special direction for 
\begin_inset Formula $\mu_{1/2}=-1$
\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
-\frac{\partial}{\partial r}\left(\psi_{1/2}\left(r\right)\right)+\sigma\left(r\right)\psi_{1/2}\left(r\right)=q_{1/2}\left(r\right),
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
which is not dependent on its neighboring half-ordinate quantities.
 Once 
\begin_inset Formula $\psi_{1/2}\left(r\right)$
\end_inset

 is calculated, 
\begin_inset Formula $\psi_{1},\psi_{3/2},\dots,\psi_{M-1/2}\dots\psi_{M}$
\end_inset

 can be calculated in that order.
 To continue the derivation, change these equations into local variables
 
\begin_inset Formula $r\in\left(0,R\right)\to\xi\in\left(-1,1\right)$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline}
\frac{2\mu_{m}}{\Delta r_{i}}\frac{\partial}{\partial\xi}\left(r^{2}\left(\xi\right)\psi_{m}\left(\xi\right)\right)+\frac{2r\left(\xi\right)}{w_{m}}\left[2\alpha_{m+1/2}\psi_{m}\left(\xi\right)-\left(\alpha_{m-1/2}+\alpha_{m-1/2}\right)\psi_{m-1/2}\left(\xi\right)\right]\\
+r^{2}\left(\xi\right)\sigma_{i}\psi_{m}\left(\xi\right)=r^{2}\left(\xi\right)q_{m}\left(\xi\right).
\end{multline}

\end_inset


\end_layout

\begin_layout Standard
Use equal weight and basis functions (the Garlerkin formulation) defined
 as
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
w_{\alpha}\left(\xi\right)=b_{\alpha}\left(\xi\right)=\frac{1}{2}\left(1+\xi_{\alpha}\xi\right),
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
with 
\begin_inset Formula $\xi_{\alpha}=\left\{ -1,1\right\} $
\end_inset

.
 Note that this 
\begin_inset Formula $\alpha$
\end_inset

 subscript is different from the 
\begin_inset Formula $\alpha$
\end_inset

 differencing coefficients.
 Use the following notation for integration over the variable 
\begin_inset Formula $\xi$
\end_inset

 inside of a cell:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\left\langle \cdot\right\rangle _{\xi}=\int_{-1}^{1}\left(\cdot\right)d\xi.
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Multiply the transport equation by weight functions 
\begin_inset Formula $w_{\beta}\left(\xi\right)$
\end_inset

 and integrate over a single spatial cell:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline}
\frac{2\mu_{m}}{\Delta r_{i}}\left\langle w_{\beta}\left(\xi\right)\frac{\partial}{\partial\xi}\left(r^{2}\left(\xi\right)\psi_{m}\left(\xi\right)\right)\right\rangle _{\xi}+\frac{2r\left(\xi\right)}{w_{m}}\left\langle w_{\beta}\left(\xi\right)\left[2\alpha_{m+1/2}\psi_{m}\left(\xi\right)-\left(\alpha_{m-1/2}+\alpha_{m-1/2}\right)\psi_{m-1/2}\left(\xi\right)\right]\right\rangle _{\xi}\\
+\left\langle w_{\beta}\left(\xi\right)r^{2}\left(\xi\right)\sigma_{i}\psi_{m}\left(\xi\right)\right\rangle _{\xi}=\left\langle w_{\beta}\left(\xi\right)r^{2}\left(\xi\right)q_{m}\left(\xi\right)\right\rangle _{\xi}.
\end{multline}

\end_inset


\end_layout

\begin_layout Standard
Integrate the first term by parts,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline}
\frac{2\mu_{m}}{\Delta r_{i}}\left[r^{2}\left(\xi\right)\psi_{m}\left(\xi\right)w_{\beta}\left(\xi\right)\right]_{\xi=-1}^{1}-\frac{2\mu_{m}}{\Delta r_{i}}\left\langle r^{2}\left(\xi\right)\psi_{m}\left(\xi\right)\frac{d}{d\xi}w_{\beta}\left(\xi\right)\right\rangle _{\xi}\\
+\frac{2}{w_{m}}\left[2\alpha_{m+1/2}\left\langle r\left(\xi\right)\psi_{m}\left(\xi\right)w_{\beta}\left(\xi\right)\right\rangle _{\xi}-\alpha_{m}\left\langle r\left(\xi\right)\psi_{m-1/2}\left(\xi\right)w_{\beta}\left(\xi\right)\right\rangle _{\xi}\right]\\
+\sigma_{i}\left\langle r^{2}\left(\xi\right)\psi_{m}\left(\xi\right)w_{\beta}\left(\xi\right)\right\rangle _{\xi}=\left\langle r^{2}\left(\xi\right)q_{m}\left(\xi\right)w_{\beta}\left(\xi\right)\right\rangle _{\xi},
\end{multline}

\end_inset


\end_layout

\begin_layout Standard
and expand 
\begin_inset Formula $\psi_{m}$
\end_inset

 in terms of its basis functions,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\psi_{m}=\sum_{\alpha=1}^{A}b_{\alpha}\left(\xi\right)\psi_{m}^{\alpha},
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
to get (with an implied sum over 
\begin_inset Formula $\psi_{m}^{\alpha}$
\end_inset

):
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline}
\overbrace{2\mu_{m}\left[r^{2}\left(\xi\right)b_{\alpha}\left(\xi\right)w_{\beta}\left(\xi\right)\right]_{\xi=-1}^{1}}^{j_{\alpha,\beta}^{r,l}}\psi_{m}^{\alpha}-\overbrace{2\mu_{m}\left\langle r^{2}\left(\xi\right)b_{\alpha}\left(\xi\right)\frac{d}{d\xi}w_{\beta}\left(\xi\right)\right\rangle _{\xi}}^{k_{\alpha,\beta}}\psi_{m}^{\alpha}\\
+\frac{2\Delta r_{i}}{w_{m}}\left[\overbrace{2\alpha_{m+1/2}\left\langle r\left(\xi\right)b_{\alpha}\left(\xi\right)w_{\beta}\left(\xi\right)\right\rangle _{\xi}}^{k_{\alpha,\beta}}\psi_{m}^{\alpha}-\overbrace{\alpha_{m}\left\langle r\left(\xi\right)b_{\alpha}\left(\xi\right)w_{\beta}\left(\xi\right)\right\rangle _{\xi}}^{\ell_{\alpha,\beta}}\psi_{m-1/2}^{\alpha}\right]\\
+\overbrace{\Delta r_{i}\left\langle r^{2}\left(\xi\right)b_{\alpha}\left(\xi\right)w_{\beta}\left(\xi\right)\right\rangle _{\xi}}^{m_{\alpha,\beta}}\sigma_{i}\psi_{m}^{\alpha}=\overbrace{\Delta r_{i}\left\langle r^{2}\left(\xi\right)b_{\alpha}\left(\xi\right)w_{\beta}\left(\xi\right)\right\rangle _{\xi}}^{m_{\alpha,\beta}}q_{m}^{\alpha},
\end{multline}

\end_inset


\end_layout

\begin_layout Standard
Define the constants
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{subequations}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
j_{\alpha,\beta,m}^{r} & =2\mu_{m}\left[r^{2}\left(\xi\right)b_{\alpha}\left(\xi\right)w_{\beta}\left(\xi\right)\right]_{\xi=1}\nonumber \\
 & =\frac{1}{8}\mu_{m}\left(\Delta r_{i}+2\bar{r}_{i}\right)^{2}\left(1+\xi_{\alpha}\right)\left(1+\xi_{\beta}\right)\\
j_{\alpha,\beta,m}^{\ell} & =-2\mu_{m}\left[r^{2}\left(\xi\right)b_{\alpha}\left(\xi\right)w_{\beta}\left(\xi\right)\right]_{\xi=-1}\nonumber \\
 & =-\frac{1}{8}\mu_{m}\left(\Delta r_{i}-2\bar{r}_{i}\right)^{2}\left(-1+\xi_{\alpha}\right)\left(-1+\xi_{\beta}\right)\\
k_{\alpha,\beta,m} & =2\mu_{m}\left\langle r^{2}\left(\xi\right)b_{\alpha}\left(\xi\right)\frac{d}{d\xi}w_{\beta}\left(\xi\right)\right\rangle _{\xi}+\frac{4\Delta r_{i}\alpha_{m+1/2}}{w_{m}}\left\langle r\left(\xi\right)b_{\alpha}\left(\xi\right)w_{\beta}\left(\xi\right)\right\rangle _{\xi}\nonumber \\
 & =\frac{1}{12w_{m}}\left[\mu_{m}w_{m}\left(\Delta r_{i}^{2}+12\bar{r}_{i}^{2}+4\Delta r_{i}\bar{r}_{i}\xi_{\alpha}\right)\xi_{\beta}+4\alpha_{m-1/2}\Delta r_{i}\left(\Delta r_{i}\left(\xi_{\alpha}+\xi_{\beta}\right)+2\bar{r}_{i}\left(3+\xi_{\alpha}\xi_{\beta}\right)\right)\right]\\
\ell_{\alpha,\beta,m} & =-\frac{2\Delta r_{i}\alpha_{m}}{w_{m}}\left\langle r\left(\xi\right)b_{\alpha}\left(\xi\right)w_{\beta}\left(\xi\right)\right\rangle _{\xi}\nonumber \\
 & =-\frac{\alpha_{m}\Delta r_{i}}{6w_{m}}\left[\Delta r_{i}\left(\xi_{\alpha}+\xi_{\beta}\right)+2\bar{r}_{i}\left(3+\xi_{\alpha}\xi_{\beta}\right)\right]\\
m_{\alpha,\beta} & =\Delta r_{i}\left\langle r^{2}\left(\xi\right)b_{\alpha}\left(\xi\right)w_{\beta}\left(\xi\right)\right\rangle _{\xi}\nonumber \\
 & =\frac{\Delta r_{i}}{120}\left[20\Delta r_{i}\bar{r}_{i}\left(\xi_{\alpha}+\xi_{\beta}\right)+20\bar{r}_{i}^{2}\left(3+\xi_{\alpha}\xi_{\beta}\right)+\Delta r_{i}^{2}\left(5+3\xi_{\alpha}\xi_{\beta}\right)\right]
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{subequations}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
and the equation becomes
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\left(f_{\alpha,\beta,m}^{r}+f_{\alpha,\beta,m}^{\ell}+k_{\alpha,\beta,m}+m_{\alpha,\beta}\sigma_{i}\right)\psi_{m}^{\alpha}+\ell_{\alpha,\beta,m}\psi_{m-1/2}^{\alpha}=m_{\alpha,\beta}\psi_{m}^{\alpha}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Note Comment
status collapsed

\begin_layout Plain Layout
\begin_inset Formula 
\begin{align*}
k_{k,j} & =\int_{-1}^{1}\frac{dh_{k}}{d\xi}r^{2}\left(\xi\right)h_{j}\left(\xi\right)d\xi\\
l_{k,j} & =\int_{-1}^{1}h_{k}\left(\xi\right)r\left(\xi\right)h_{j}\left(\xi\right)d\xi\\
m_{k,j} & =\int_{-1}^{1}h_{k}\left(\xi\right)r^{2}\left(\xi\right)h_{j}\left(\xi\right)d\xi
\end{align*}

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\begin{align*}
\bm{K} & =\begin{bmatrix}\left(-\frac{\bar{r}_{i}^{2}}{2}+\frac{\bar{r}_{i}\Delta r_{i}}{6}-\frac{\Delta r_{i}^{2}}{24}\right) & \left(-\frac{\bar{r}_{i}^{2}}{2}-\frac{\bar{r}_{i}\Delta r_{i}}{6}-\frac{\Delta r_{i}^{2}}{24}\right)\\
\left(\frac{\bar{r}_{i}^{2}}{2}-\frac{\bar{r}_{i}\Delta r_{i}}{6}+\frac{\Delta r_{i}^{2}}{24}\right) & \left(\frac{\bar{r}_{i}^{2}}{2}+\frac{\bar{r}_{i}\Delta r_{i}}{6}+\frac{\Delta r_{i}^{2}}{24}\right)
\end{bmatrix}\\
\bm{L} & =\begin{bmatrix}\left(\frac{2\bar{r}_{i}}{3}-\frac{\Delta r_{i}}{6}\right) & \left(\frac{\bar{r}_{i}}{3}\right)\\
\left(\frac{\bar{r}_{i}}{3}\right) & \left(\frac{2\bar{r}_{i}}{3}+\frac{\Delta r_{i}}{6}\right)
