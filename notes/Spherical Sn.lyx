k_{\alpha,\beta,\pm} & =-\left\langle \mu\left(\eta\right)r^{2}\left(\xi\right)b_{\alpha}\left(\xi,\eta\right)\frac{\partial}{\partial\xi}w_{\beta}\left(\xi,\eta\right)\right\rangle _{\xi,\eta}-\Delta r_{i}\left\langle r\left(\xi\right)\left(1-\mu^{2}\left(\eta\right)\right)b_{\alpha}\left(\xi,\eta\right)\frac{\partial}{\partial\eta}w_{\beta}\left(\xi,\eta\right)\right\rangle _{\xi,\eta}\nonumber \\
 & =-\frac{1}{288}\left(\eta_{\alpha}+\eta_{\beta}+3\eta_{\pm}+\eta_{\alpha}\eta_{\beta}\eta_{\pm}\right)\left(\Delta r_{i}^{2}+12\bar{r}_{i}^{2}+4\Delta r_{i}\bar{r}_{i}\xi_{\alpha}\right)\xi_{\beta}\nonumber \\
 & \qquad+\frac{\Delta r_{i}}{288}\eta_{\beta}\left(-11+2\eta_{\alpha}\eta_{\pm}+3\eta_{\pm}^{2}\right)\left(\Delta r_{i}\left(\xi_{\alpha}+\xi_{\beta}\right)+2\bar{r}_{i}\left(3+\xi_{\alpha}\xi_{\beta}\right)\right)\nonumber \\
 & =-\frac{1}{288}\left(\eta_{\alpha}+\eta_{\beta}+3\eta_{\pm}+\eta_{\alpha}\eta_{\beta}\eta_{\pm}\right)\left(\Delta r_{i}^{2}+12\bar{r}_{i}^{2}+4\Delta r_{i}\bar{r}_{i}\xi_{\alpha}\right)\xi_{\beta}\nonumber \\
 & \qquad+\frac{\Delta r_{i}}{288}\eta_{\beta}\left(-8+2\eta_{\alpha}\eta_{\pm}\right)\left(\Delta r_{i}\left(\xi_{\alpha}+\xi_{\beta}\right)+2\bar{r}_{i}\left(3+\xi_{\alpha}\xi_{\beta}\right)\right),\\
\ell_{\alpha,\beta,\pm} & =\Delta r_{i}\left\langle r\left(\xi\right)\left[w_{\beta}\left(\xi,\eta\right)\left(1-\mu^{2}\left(\eta\right)\right)b_{\alpha}\left(\xi,\eta\right)\right]_{\eta=-1}^{1}\right\rangle _{\xi}\nonumber \\
 & =-\frac{\Delta r_{i}}{96}\left(2\eta_{\pm}+\eta_{\beta}\left(-3+\eta_{\pm}^{2}\right)+\eta_{\alpha}\left(-3+2\eta_{\beta}\eta_{\pm}+\eta_{\pm}^{2}\right)\right)\left(\Delta r_{i}\left(\xi_{\alpha}+\xi_{\beta}\right)+2\bar{r}_{i}\left(3+\xi_{\alpha}\xi_{\beta}\right)\right)\nonumber \\
 & =-\frac{\Delta r_{i}}{96}\left(2\eta_{\pm}-2\eta_{\beta}+2\eta_{\alpha}\left(-1+\eta_{\beta}\eta_{\pm}\right)\right)\left(\Delta r_{i}\left(\xi_{\alpha}+\xi_{\beta}\right)+2\bar{r}_{i}\left(3+\xi_{\alpha}\xi_{\beta}\right)\right)\\
m_{\alpha,\beta} & =\left(\frac{\Delta r_{i}}{2}\right)\sigma_{i}\left\langle r^{2}\left(\xi\right)w_{\beta}\left(\xi,\eta\right)b_{\alpha}\left(\xi,\eta\right)\right\rangle _{\xi,\eta}\nonumber \\
 & =\frac{\Delta r_{i}}{1440}\left[\left(3+\eta_{\alpha}\eta_{\beta}\right)\left(20\Delta r_{i}\bar{r}_{i}\left(\xi_{\alpha}+\xi_{\beta}\right)+20\bar{r}_{i}^{2}\left(3+\xi_{\alpha}\xi_{\beta}\right)+\Delta r_{i}^{2}\left(5+3\xi_{\alpha}\xi_{\beta}\right)\right)\right],
\end{align}

